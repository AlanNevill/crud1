{
    "extends": ["eslint:recommended", "prettier"],
    "parser": "babel-eslint",
    "env": {
        "amd": true,
        "browser": true,
        "jquery": true,
        "node": true,
        "es6": true,
        "worker": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "impliedStrict": true,
        "sourceType": "module"
    },
    "plugins": ["prettier"],

    "rules": {
        "eqeqeq": [2, "smart"],
        "no-console": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "valid-jsdoc": [
            2,
            {
                "requireReturn": true
            }
        ]
    }
}
